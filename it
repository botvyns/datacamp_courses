[32m+[m[32m    "X_train, X_test, y_train, y_test = train_test_split(filtered_text.toarray(), y, stratify=y, random_state=42)\n",[m
[32m+[m[32m    "\n",[m
[32m+[m[32m    "nb.fit(X_train, y_train)\n",[m
[32m+[m[32m    "\n",[m
[32m+[m[32m    "nb.score(X_test, y_test)"[m
